se) => {\n    const { instanceName, routeId, pluginId } = req.params;\n    const { config } = req.body;\n\n    if (!(await this.isRequestAuthorized(req, kongUpdateRoutePluginPermission ))) {\n      throw new NotAllowedError('Unauthorized');\n    }\n\n\n    try {\n      const response = await this.kongServiceManagerApi\n      .editRoutePlugin(\n        instanceName,\n        routeId,\n        pluginId,\n        config,\n      );\n\n      res.status(200).send(response);\n\n    } catch (err: any) {\n      if (err.errors) {\n        throw new InputError(\n          `Unable to edit plugin in route [${routeId}]: ${stringifyError(\n            err.errors,\n          )}`,\n        );\n      }\n      throw err;\n    }\n  };\n\n  removeRoutePlugin = async (req: Request, res: Response) => {\n    const { instanceName, routeId,pluginId } = req.params;\n\n    if (!(await this.isRequestAuthorized(req, kongDisableRoutePluginPermission ))) {\n      throw new NotAllowedError('Unauthorized');\n    }\n\n    try {\n      const response = await this.kongServiceManagerApi\n      .removeRoutePlugin(\n        instanceName,\n        routeId,\n        pluginId,\n      );\n        res.status(204).json(response);\n\n    } catch (err: any) {\n      if (err.errors) {\n        throw new InputError(\n          `Unable to delete plugin [${pluginId}] in route [${routeId}]: ${stringifyError(\n            err.errors,\n          )}`,\n        );\n      }\n      throw err;\n    }\n  };\n\n}"],"names":["KongController","kongRoutesReadPermission","NotAllowedError","InputError","stringifyError","kongRouteCreatePermission","kongUpdateRoutePermission","kongRouteDeletePermission","kongApplyPluginToRoutePermission","kongUpdateRoutePluginPermission","kongDisableRoutePluginPermission"],"mappings":";;;;;;AAOO,MAAM,yBAA0BA,6BAA2C,CAAA;AAAA,EAEhF,SAAA,GAAY,OAAO,GAAA,EAAa,GAAiB,KAAA;AAC/C,IAAA,MAAM,EAAE,YAAA,EAAc,WAAY,EAAA,GAAI,GAAI,CAAA,MAAA;AAE1C,IAAA,IAAI,CAAE,MAAM,IAAA,CAAK,mBAAoB,CAAA,GAAA,EAAKC,gEAAwB,CAAI,EAAA