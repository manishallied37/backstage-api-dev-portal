tItemText className={itemValue}>\r\n                  {serviceDetails.protocol ?? \" - \"}\r\n                </ListItemText>\r\n                </Box>\r\n              </ListItem>\r\n              {/* HOST */}\r\n              <ListItem className={listItemWrapper}>\r\n                <Box className={listItem}>\r\n                  <LabelField title=\"Host\"/>\r\n                <ListItemText className={itemValue}>\r\n                  {serviceDetails.host ?? \" - \"}\r\n                </ListItemText>\r\n                </Box>\r\n              </ListItem>\r\n              {/* PATH */}\r\n              <ListItem className={listItemWrapper}>\r\n                <Box className={listItem}>\r\n                  <LabelField title=\"Path\"/>\r\n                <ListItemText className={itemValue}>\r\n                  {serviceDetails.path ?? \" - \"}\r\n                </ListItemText>\r\n                </Box>\r\n              </ListItem>\r\n              {/* PORT */}\r\n              <ListItem className={listItemWrapper}>\r\n                <Box className={listItem}>\r\n                  <LabelField title=\"Port\"/>\r\n                <ListItemText className={itemValue}>\r\n                  {serviceDetails.port ?? \" - \"}\r\n                </ListItemText>\r\n                </Box>\r\n              </ListItem>\r\n              {/* TAGS */}\r\n              <ListItem className={listItemWrapper}>\r\n                <Box className={listItem}>\r\n                  <LabelField title=\"Tags\"/>\r\n                <ListItemText className={itemValue}>\r\n                  { serviceDetails.tags && serviceDetails.tags.length > 0 ? (serviceDetails.tags.map(t=>(\r\n                    <Chip label={t} key={t}/>\r\n                  ))): \" - \" }\r\n                </ListItemText>\r\n                </Box>\r\n              </ListItem>\r\n            </>\r\n              ) : (\r\n                <ListItem>\r\n                  <ListItemText>No records to display</ListItemText>\r\n    