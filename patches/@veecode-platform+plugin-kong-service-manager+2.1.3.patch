-{"version":3,"file":"PluginsList.esm.js","sources":["../../../src/components/PluginsList/PluginsList.tsx"],"sourcesContent":["import React from 'react'\nimport { BoxComponent, EmptyStateComponent } from '../shared'\nimport { PluginsCards } from './PluginsCards';\nimport useAsync from 'react-use/lib/useAsync';\nimport { CardTab, TabbedCard } from '@backstage/core-components';\nimport ErrorBoundary from '../ErrorBoundary/ErrorBondary';\nimport { usePluginListStyles } from './styles';\nimport { Box } from '@material-ui/core';\nimport { useKongServiceManagerContext } from '../../context';\nimport { PluginListProps } from './types';\nimport { PluginListSkeleton } from './PluginListSkeleton';\n\n\n\nconst PluginsList : React.FC<PluginListProps> = (props) => {\n\n  const {listAllPlugins, listAssociatedPlugins, associatedPluginsState, associatedPluginsName } = props;\n  const { wrapper, emptyContent } = usePluginListStyles();\n  const { pluginsPerCategoryState } = useKongServiceManagerContext();\n\n  const getPluginsEnabled = async () => {\n    await listAllPlugins();\n  };\n\n    const { loading, error } = useAsync(async (): Promise<void> => {\n      await Promise.all([\n           Promise.all([listAllPlugins(), listAssociatedPlugins()])\n      ]);\n    }, []);\n\n  React.useEffect(()=>{\n    getPluginsEnabled();\n  // eslint-disable-next-line react-hooks/exhaustive-deps\n  },[associatedPluginsState]);\n\n  if(error) return <EmptyStateComponent/>;\n\n  return (\n    <ErrorBoundary>\n      <BoxComponent title=\"Kong Plugins\" searchBar>\n        <Box className={wrapper}>\n          <TabbedCard title=\"\">\n            <CardTab label=\"All Plugins\">\n              {loading && (<PluginListSkeleton/>)}\n              {pluginsPerCategoryState && pluginsPerCategoryState.length >=1 ? <PluginsCards listAllPlugins={listAllPlugins} associatedPluginsName={associatedPluginsName} /> : <div className={emptyContent}> No data to display ...</div>}\n            </CardTab>\n              <CardTab \n               label=\"Associated Plugins\">\n                {loading && (<PluginListSkeleton/>)}\n                {associatedPluginsState && associatedPluginsState.length >= 1 ? (<PluginsCards filterByAssociated listAllPlugins={listAllPlugins} associatedPluginsName={associatedPluginsName} />) \n                : <div className={emptyContent}> No data to display ...</div>}\n              </CardTab>\n          </TabbedCard>\n        </Box>\n      </BoxComponent>\n    </ErrorBoundary>\n  );\n}\n\nexport default React.memo(PluginsList);"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAcA,MAAM,WAAA,GAA0C,CAAC,KAAA,KAAU;AAEzD,EAAA,MAAM,EAAC,cAAA,EAAgB,qBAAA,EAAuB,sBAAA,EAAwB,uBAAsB,GAAI,KAAA;AAChG,EAAA,MAAM,EAAE,OAAA,EAAS,YAAA,EAAa,GAAI,mBAAA,EAAoB;AACtD,EAAA,MAAM,EAAE,uBAAA,EAAwB,GAAI,4BAAA,EAA6B;AAEjE,EAAA,MAAM,oBAAoB,YAAY;AACpC,IAAA,MAAM,cAAA,EAAe;AAAA,GACvB;AAEE,EAAA,MAAM,EAAE,OAAA,EAAS,KAAA,EAAM,GAAI,SAAS,YAA2B;AAC7D,IAAA,MAAM,QAAQ,GAAA,CAAI;AAAA,MACb,QAAQ,GAAA,CAAI,CAAC,gBAAe,EAAG,qBAAA,EAAuB,CAAC;AAAA,KAC3D,CAAA;AAAA,GACH,EAAG,EAAE,CAAA;AAEP,EAAA,KAAA,CAAM,UAAU,MAAI;AAClB,IAAA,iBAAA,EAAkB;AAAA,GAEpB,EAAE,CAAC,sBAAsB,CAAC,CAAA;AAE1B,EAAA,IAAG,KAAA,EAAO,uBAAO,KAAA,CAAA,aAAA,CAAC,mBAAA,EAAA,IAAmB,CAAA;AAErC,EAAA,2CACG,aAAA,EAAA,IAAA,kBACC,KAAA,CAAA,aAAA,CAAC,YAAA,EAAA,EAAa,KAAA,EAAM,gBAAe,SAAA,EAAS,IAAA,EAAA,kBAC1C,KAAA,CAAA,aAAA,CAAC,GAAA,EAAA,EAAI,WAAW,OAAA,EAAA,kBACd,KAAA,CAAA,aAAA,CAAC,cAAW,KAAA,EAAM,EAAA,EAAA,sCACf,OAAA,EAAA,EAAQ,KAAA,EAAM,aAAA,EAAA,EACZ,OAAA,wCAAa,kBAAA,EAAA,IAAkB,CAAA,EAC/B,2BAA2B,uBAAA,CAAwB,MAAA,IAAS,oBAAI,KAAA,CAAA,aAAA,CAAC,YAAA,EAAA,EAAa,cAAA,EAAgC,qBAAA,EAA8C,oBAAK,KAAA,CAAA,aAAA,CAAC,KAAA,EAAA,EAAI,WAAW,YAAA,EAAA,EAAc,yBAAuB,CACzN,CAAA,kBACE,KAAA,CAAA,aAAA;AAAA,IAAC,OAAA;AAAA,IAAA;AAAA,MACA,KAAA,EAAM;AAAA,KAAA;AAAA,IACJ,OAAA,wCAAa,kBAAA,EAAA,IAAkB,CAAA;AAAA,IAC/B,sBAAA,IAA0B,sBAAA,CAAuB,MAAA,IAAU,CAAA,uCAAM,YAAA,EAAA,EAAa,kBAAA,EAAkB,IAAA,EAAC,cAAA,EAAgC,uBAA8C,CAAA,mBAC9K,KAAA,CAAA,aAAA,CAAC,KAAA,EAAA,EAAI,SAAA,E